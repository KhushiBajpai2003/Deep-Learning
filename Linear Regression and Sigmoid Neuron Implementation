ef train(self,input,target,epochs=50,lr=0.01):\n","    y_pred = self.linear_regression(input)\n","\n","    #calculate loss\n","    error = y_pred - target\n","\n","    #Gradient descent to update weight and bias\n","    self.w -=lr*np.dot(error,input)/len(input)\n","    self.b-=lr*np.mean(error)\n","\n","  def predict(self,x):\n","    return self.linear_regression(x)\n"],"metadata":{"id":"yFFEYhE2Rjbd","executionInfo":{"status":"ok","timestamp":1707976237960,"user_tz":-330,"elapsed":687,"user":{"displayName":"D_09_ Sakshi Fadnavis","userId":"03529767819078202026"}}},"execution_count":11,"outputs":[]},{"cell_type":"code","source":["neuron = Neuron()"],"metadata":{"id":"kPcCt_qORsI6","executionInfo":{"status":"ok","timestamp":1707976254010,"user_tz":-330,"elapsed":2,"user":{"displayName":"D_09_ Sakshi Fadnavis","userId":"03529767819078202026"}}},"execution_count":12,"outputs":[]},{"cell_type":"code","source":["neuron.train(input,target)"],"metadata":{"id":"33qBjCfrUVwS","executionInfo":{"status":"ok","timestamp":1707976320626,"user_tz":-330,"elapsed":783,"user":{"displayName":"D_09_ Sakshi Fadnavis","userId":"03529767819078202026"}}},"execution_count":13,"outputs":[]},{"cell_type":"code","source":["internal_marks = 0.15"],"metadata":{"id":"qJQxFa3PUm67","executionInfo":{"status":"ok","timestamp":1707976339588,"user_tz":-330,"elapsed":3,"user":{"displayName":"D_09_ Sakshi Fadnavis","userId":"03529767819078202026"}}},"execution_count":14,"outputs":[]},{"cell_type":"code","source":["predicted_ext_marks = neuron.predict(internal_marks)\n","print(\"Predicted external marks for internal marks 0.15:\", predicted_ext_marks)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"elMpCeTuUqtq","executionInfo":{"status":"ok","timestamp":1707976393843,"user_tz":-330,"elapsed":3,"user":{"displayName":"D_09_ Sakshi Fadnavis","userId":"03529767819078202026"}},"outputId":"7b7f488a-402f-4f16-8b44-ba4509179285"},"execution_count":16,"outputs":[{"output_type":"stream","nam